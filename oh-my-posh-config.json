{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/v1/schemas/current.schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#800080",
          "template": "{{ .UserName }} "
        },
        {
          "type": "path",
          "style": "powerline",
          "foreground": "#ffffff",
          "background": "#293344",
          "properties": {
            "folder_separator_icon": " \uE0B1 ",
            "style": "folder"
          },
          "template": " {{ .Path }} "
        },
        {
          "type": "git",
          "style": "powerline",
          "foreground": "#ffffff",
          "background": "#FF7F00",
          "properties": {
            "branch_icon": "\uE0A0 ",
            "fetch_status": true,
            "fetch_upstream_status": true
          },
          "template": " {{ .BranchStatus }}{{ .Branch }} {{ .WorkingStatus }}"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#FF0000",
          "template": "\u276F "
        }
      ]
    }
  ],
  "version": 2
}
